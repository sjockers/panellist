<li class="panel {{ active }}" style="{{ model.position.styleDefinition }}">
  <div class="panel__inner" style="
    background: {{ model.background }};
    opacity: {{ opacity }};
    top: {{ top }}px;
    right: {{ right }}px;">

    <img src='{{ model.image }}' />
    <div data-hook="subpanels"></div>
  </div>
</li>
